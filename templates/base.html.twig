        <!-- Bootstrap Core JavaScript -->
        <script src="{{ asset('build/vendor/bootstrap/js/bootstrap.min.js') }}"></script>
        <!-- Nav JavaScript -->
        <script src="{{ asset('build/js/awesomenav.js') }}"></script>

        <!-- jQuery -->
        <!--<script src="vendor/masterslider/jquery.min.js"></script>-->
